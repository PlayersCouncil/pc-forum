{
    "name": "stoker/debug",
    "type": "phpbb-extension",
    "description": "Enable Debug and Display load time and errors.",
    "homepage": "https://phpbb3bbcodes.com",
    "version": "1.0.0",
    "time": "2025-02-19",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Stoker",
            "email": "",
            "homepage": "https://phpbb3bbcodes.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
    },
    "extra": {
        "display-name": "Enable Debug",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/enable_debug",
            "filename": "version_check",
            "ssl": true
        }
    }
}